
typedef struct student{
	string student_first_name[30];
	string student_last_name[30];
    int student_age;
    float student_mark;
    string student_group[20];
    struct student *next_s;
	}student;
	
 typedef struct teacher{
   string teacher_name[30];
   string teacher_subject[40];
   struct teacher *next_t;
 }teacher;
 
